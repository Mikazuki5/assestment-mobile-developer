{
 "extends": "@tsconfig/react-native/tsconfig.json",
 "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@navigation/*": ["src/navigators/*"],
      "@screens/*": ["src/screens/*"],
